<p id="notice"><%= notice %></p>

<h1>Ordens</h1>

<table>
  <thead>
    <tr>
      <th>Cliente</th>
      <th>Codigo</th>
      <th>Proceso</th>
      <th>Entrega</th>
      <th>Cierre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ordens.each do |orden| %>
      <tr>
        <td><%= orden.cliente_id %></td>
        <td><%= orden.codigo %></td>
        <td><%= orden.proceso %></td>
        <td><%= orden.entrega %></td>
        <td><%= orden.cierre %></td>
        <td><%= link_to 'Show', orden %></td>
        <td><%= link_to 'Edit', edit_orden_path(orden) %></td>
        <td><%= link_to 'Destroy', orden, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Orden', new_orden_path %>
